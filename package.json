{
  "name": "gidevo-api-tool",
  "version": "0.1.3",
  "description": "Enterprise-grade API integration & code generation CLI with plugin architecture.",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gidevo-api-tool": "dist/cli/index.js"
  },
  "files": [
    "dist/**/*",
    "LICENSE*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lehrosolutions/gidevo-api-tool.git"
  },
  "bugs": {
    "url": "https://github.com/lehrosolutions/gidevo-api-tool/issues"
  },
  "homepage": "https://github.com/lehrosolutions/gidevo-api-tool#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "prepare": "npm run build",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags",
    "release": "semantic-release"
  },
  "keywords": [
    "api",
    "integration",
    "openapi",
    "graphql",
    "sdk",
    "codegen",
    "cli",
    "developer-tools"
  ],
  "author": "LEHRO Solutions",
  "license": "(Apache-2.0 OR LicenseRef-LEHRO-Solutions-Commercial)",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.0",
    "semantic-release": "^20.0.0"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "graphql": "^16.0.0",
    "inquirer": "^9.0.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.0.0",
    "ora": "^6.1.2"
  }
}
